table.insert(cors,sandbox(Script1174,function()
local speedBoost = script.Parent

local function steppedOn(part)
	local parent = part.Parent
	if game.Players:GetPlayerFromCharacter(parent) then
		parent.Humanoid.WalkSpeed = 5
		
	end
end

speedBoost.Touched:connect(steppedOn)
end))
Script1175.Name = "Damage Script"
Script1175.Parent = MeshPart1167
table.insert(cors,sandbox(Script1175,function()
function onTouched(hit)
        local human = hit.Parent:findFirstChild("Humanoid")
        if (human ~= nil) then
                human.Health = human.Health - 7.5 -- Change the amount to change the damage.
        end
end
script.Parent.Touched:connect(onTouched)
end))
Script1176.Name = "trip_brick"
Script1176.Parent = MeshPart1167
table.insert(cors,sandbox(Script1176,function()
b = script.Parent
b.Touched:connect(function(hit)
	local c = hit.Parent
	if c then
		for _,h in next, c:GetChildren() do
			if h:IsA'Humanoid' and h.Health > 0 and h:GetState().Name ~= 'Physics' then
				
				--if h.WalkSpeed > 22 then break end
				--^ ignore unless you're explode1.  ~mael
				
				local newscript = script.physics:Clone()
				newscript.Parent = c
				newscript.Disabled = false
				--h:ChangeState'Physics'
				
				wait(4)
				
				if h.Parent then
					local newscript = script.gettingup:Clone()
					newscript.Parent = c
					newscript.Disabled = false
					--h:ChangeState'GettingUp'
				end
				break
			end
		end
	end
end)
end))
