	'WE ALL FLOAT DOWN HERE!',
	'BEEP BEEP RICHIE',
	'do you want a balloon.',
    "You'll float too.",
	'HAHAHAHAHA!'
}

local distance = 100
local detectdistance = 80
local detectspeed = 10

