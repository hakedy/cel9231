if BaeTarget then
for _,Child in pairs(BaeTarget.Parent:GetChildren())do
if Child.ClassName=="Humanoid"then
FoundHumanoid=Child;
end;
end;
end;
if BaeTarget and BaeTarget.Parent and FoundHumanoid and FoundHumanoid.Jump then
MarioHumanoid.Jump=true;
end;
